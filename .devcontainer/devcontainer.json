{
    "name": "Ubuntu Web Lab",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "ubuntu-web",
    "workspaceFolder": "/home/ubuntuweb/iso-builder",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "ms-ceintl.vscode-language-pack-fr"
            ]
        }
    },
    "remoteUser": "ubuntuweb",
    "portsAttributes": {
        "6080": {
            "label": "Ubuntu Web Desktop",
            "onAutoForward": "openBrowser"
        }
    }
}
