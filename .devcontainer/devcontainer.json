{
    "name": "Windows Lab (Virtualizer)",
    "build": {
        "dockerfile": "../Dockerfile",
        "context": ".."
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker"
            ]
        }
    },
    "remoteUser": "root",
    "forwardPorts": [8080],
    "portsAttributes": {
        "8080": {
            "label": "Windows Lab UI",
            "onAutoForward": "openBrowser"
        }
    }
}
